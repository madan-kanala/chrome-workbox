{
  "extends": ["eslint:recommended", "google"],
  "env": {
    "serviceworker": true,
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "valid-jsdoc": 1,
    "require-jsdoc": 1
  },
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "globals": {
    "BroadcastChannel": false
  }
}
