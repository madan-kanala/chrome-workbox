importScripts('{{CDN_URL}}/workbox-sw.dev.js');

const wb = new WorkboxSW({
  debug: true
});

wb.core.setLogLevel(workbox.core.LOG_LEVELS.debug);

const printLogs = () => {
  // ‚ò†Ô∏è You should never so this - this is just so we can show off our logging.
  workbox.core._private.logger.debug(`üêõ Oh hai! I'm a debug message.`);
  workbox.core._private.logger.log(`üîß Good ole log message.`);
  workbox.core._private.logger.warn(`‚ö†Ô∏è Uh Oh.... I'm a warning.`);
  workbox.core._private.logger.error(`‚ò†Ô∏è Eek, stuff is breaking. p.s. I'm an error.`);
};

self.addEventListener('message', function(event) {
  switch(event.data.command) {
    case 'printLogs':
    printLogs();
      break;
    default:
      console.log(`Unknown command received in the service worker: `, event);
  }
});
