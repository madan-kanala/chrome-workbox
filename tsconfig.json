{
  "compilerOptions": {
    "lib": ["es2017", "webworker"],
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "composite": true,
    "declaration": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
