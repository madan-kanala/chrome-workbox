{
  "name": "workbox",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "bot": "pr-bot",
    "coverage-report": "nyc report --reporter lcov --reporter text",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "demos-deploy": "firebase deploy --project workbox-demos",
    "demos-serve": "firebase serve --only hosting,functions --project workbox-demos",
    "lint": "npm run lint-js && npm run lint-ts",
    "lint-js": "eslint '**/*.{js,mjs}' --config javascript.eslintrc.js --ignore-path .gitignore",
    "lint-ts": "eslint '**/*.ts' --config typescript.eslintrc.js --ignore-path .gitignore",
    "local-jsdoc": "jsdoc",
    "local-lerna": "lerna",
    "local-tsc": "tsc",
    "test": "nyc --clean false --silent mocha --timeout 60000",
    "version": "git add -A packages"
  },
  "nyc": {
    "include": [
      "packages/**/*"
    ],
    "extension": [
      ".mjs",
      ".js"
    ],
    "exclude": [
      "packages/workbox-core/models/messages/messages.mjs"
    ]
  },
  "pre-push": [
    "lint"
  ],
  "version": "0.0.0",
  "description": "Top-level scripts and dependencies for the workbox monorepo. Not meant to be published to npm.",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoogleChrome/workbox.git"
  },
  "author": "Google's Web DevRel Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GoogleChrome/workbox/issues"
  },
  "homepage": "https://github.com/GoogleChrome/workbox#readme",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@google-cloud/storage": "^4.3.0",
    "@octokit/rest": "^16.43.1",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "acorn": "^7.1.0",
    "archiver": "^3.1.1",
    "babel-plugin-transform-async-to-promises": "^0.8.15",
    "babylon": "^6.18.0",
    "body-parser": "^1.19.0",
    "bytes": "^3.1.0",
    "camelcase": "^5.3.1",
    "chai": "^4.2.0",
    "chalk": "^3.0.0",
    "clear-require": "^3.0.0",
    "comlinkjs": "^3.2.0",
    "common-tags": "^1.8.0",
    "copy-webpack-plugin": "^5.1.1",
    "coveralls": "^3.0.9",
    "del": "^5.1.0",
    "depcheck": "^0.9.2",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-jsdoc": "^21.0.0",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "globby": "^11.0.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gzip-size": "^5.1.1",
    "html-webpack-plugin": "^3.2.0",
    "jsdoc": "^3.6.3",
    "jsdoc-baseline": "^0.1.5",
    "lerna": "^3.20.2",
    "memory-fs": "^0.5.0",
    "minimist": "^1.2.0",
    "mocha": "^7.0.1",
    "nunjucks": "^3.2.0",
    "nyc": "^15.0.0",
    "pre-push": "^0.1.1",
    "proxyquire": "^2.1.3",
    "require-dir": "^1.2.0",
    "rollup": "^1.31.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-multi-entry": "^2.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-typescript2": "^0.25.3",
    "selenium-assistant": "^6.0.0",
    "semver": "^7.1.2",
    "serve-index": "^1.9.1",
    "service-worker-mock": "^1.9.3",
    "sinon": "^8.1.1",
    "tempy": "^0.3.0",
    "typescript": "^3.7.5",
    "upath": "^1.2.0",
    "url-search-params": "^1.1.0",
    "webpack": "^4.41.6",
    "worker-plugin": "^3.2.0"
  }
}
