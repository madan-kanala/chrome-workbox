{
  "name": "workbox",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "local-lerna": "lerna",
    "test": "nyc --require @std/esm --require ./infra/utils/sw-env --reporter=lcov --reporter=text mocha",
    "lint": "eslint \"**/*.{js,mjs}\" --ignore-path .gitignore",
    "bot": "pr-bot",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "@std/esm": {
    "cjs": true
  },
  "nyc": {
    "include": [
      "packages/**/*"
    ],
    "extension": [
      ".mjs",
      ".js"
    ]
  },
  "pre-push": [
    "lint"
  ],
  "version": "0.0.0",
  "description": "Top-level scripts and dependencies for the workbox monorepo. Not meant to be published to npm.",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoogleChrome/workbox.git"
  },
  "author": "Google's Web DevRel Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/GoogleChrome/workbox/issues"
  },
  "homepage": "https://github.com/GoogleChrome/workbox#readme",
  "devDependencies": {
    "@google-cloud/storage": "^1.2.1",
    "@std/esm": "^0.5.1",
    "archiver": "^2.0.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babylon": "^6.18.0",
    "browser-sync": "^2.18.13",
    "bytes": "^3.0.0",
    "chai": "^4.1.2",
    "chalk": "^2.1.0",
    "chromedriver": "^2.32.3",
    "clear-require": "^2.0.0",
    "common-tags": "^1.4.0",
    "cookie-parser": "^1.4.3",
    "coveralls": "^2.13.1",
    "depcheck": "^0.6.7",
    "eslint": "^4.6.1",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-jsdoc": "^3.1.3",
    "event-stream": "^3.3.4",
    "express": "^4.15.4",
    "fs-extra": "^4.0.1",
    "geckodriver": "^1.8.1",
    "github": "^11.0.0",
    "glob": "^7.1.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-babel": "^7.0.0",
