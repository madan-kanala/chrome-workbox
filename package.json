{
  "name": "workbox",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "local-jsdoc": "jsdoc",
    "local-lerna": "lerna",
    "test": "nyc --clean false --require @std/esm --require ./infra/testing/sw-env --silent mocha ./infra/testing/auto-stub-logger.mjs",
    "coverage-report": "nyc report --reporter lcov --reporter text",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "eslint \"**/*.{js,mjs}\" --ignore-path .gitignore",
    "bot": "pr-bot",
    "demos-serve": "firebase serve --only hosting,functions --project workbox-demos",
    "demos-deploy": "firebase deploy --project workbox-demos"
  },
  "@std/esm": {
    "cjs": true
  },
  "nyc": {
    "include": [
      "packages/**/*"
    ],
    "extension": [
      ".mjs",
      ".js"
    ],
    "exclude": [
      "packages/workbox-core/models/messages/messages.mjs"
    ]
  },
  "pre-push": [
    "lint"
  ],
  "version": "0.0.0",
  "description": "Top-level scripts and dependencies for the workbox monorepo. Not meant to be published to npm.",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoogleChrome/workbox.git"
  },
  "author": "Google's Web DevRel Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/GoogleChrome/workbox/issues"
  },
  "homepage": "https://github.com/GoogleChrome/workbox#readme",
  "devDependencies": {
    "@google-cloud/storage": "^1.2.1",
    "@std/esm": "^0.5.1",
    "archiver": "^2.0.3",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babylon": "^6.18.0",
    "browser-sync": "^2.18.13",
    "bytes": "^3.0.0",
    "chai": "^4.1.2",
    "chalk": "^2.1.0",
    "chromedriver": "^2.33.2",
    "clear-require": "^2.0.0",
    "common-tags": "^1.4.0",
    "cookie-parser": "^1.4.3",
    "copy-webpack-plugin": "^4.2.0",
    "coveralls": "^3.0.0",
    "depcheck": "^0.6.7",
    "eslint": "^4.6.1",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-header": "^1.2.0",
    "eslint-plugin-jsdoc": "^3.1.3",
    "event-stream": "^3.3.4",
    "express": "^4.16.1",
    "firebase-tools": "^3.13.1",
    "fs-extra": "^4.0.1",
    "geckodriver": "^1.8.1",
    "github": "^12.0.1",
    "glob": "^7.1.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-babel": "^7.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.6.1",
    "gzip-size": "^4.0.0",
    "jsdoc": "^3.5.5",
    "lerna": "^2.4.0",
    "mocha": "^4.0.1",
    "nyc": "^11.2.1",
    "path-extra": "^4.2.1",
    "pr-bot": "0.2.0",
    "pre-push": "^0.1.1",
    "proxyquire": "^1.8.0",
    "rollup": "^0.49.3",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-istanbul": "^1.1.0",
    "rollup-plugin-multi-entry": "^2.0.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-plugin-uglify-es": "0.0.1",
    "rollup-stream": "^1.24.1",
    "selenium-assistant": "^5.2.0",
    "semver": "^5.4.1",
    "serve-index": "^1.9.1",
    "serve-static": "^1.13.1",
    "service-worker-mock": "^1.7.2",
    "shelving-mock-indexeddb": "philipwalton/shelving-mock-indexeddb#clone-fix",
    "sinon": "^4.0.2",
    "tempy": "^0.2.1",
    "uglify-es": "^3.1.0",
    "url-search-params": "^0.10.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "^3.8.1"
  }
}
